{
  "name": "Mind Sport",
  "description": "Mind Sport Browser Extension",
  "permissions": [
    "storage"
  ],
  "version": "1.0",
  "manifest_version": 2,
  "browser_action": {
    "default_popup": "hello.html",
    "default_icon": "icons/icon-32.png"
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "windows": "Ctrl+Shift+Y",
        "mac": "Command+Shift+Y",
        "chromeos": "Ctrl+Shift+U",
        "linux": "Ctrl+Shift+J"
      },
      "description": "Opens hello.html"
    }
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  }
}